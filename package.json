{
  "name": "@fifthrevision/axle",
  "version": "0.7.0-beta",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johncch/axle.git"
  },
  "description": "axle is a command line tool for running workflows against LLM APIs.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "axle": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "start": "tsx ./src/cli.ts",
    "build": "pkgroll --clean-dist --minify",
    "build:watch": "pkgroll --watch",
    "build-dev": "pkgroll --clean-dist",
    "test": "vitest run",
    "get-models": "npx tsx scripts/getModels.ts",
    "prompt-scenarios": "npx tsx ./scripts/prompt.scenarios.ts",
    "test-providers": "npx tsx ./scripts/testProviders.ts",
    "sanity-check": "npx tsx ./scripts/sanity-check.ts",
    "release": "npm test && npm run build && npm version"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@commander-js/extra-typings": "^14.0.0",
    "@google/genai": "^1.40.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "glob": "^13.0.1",
    "mime": "^4.1.0",
    "openai": "^6.19.0",
    "serialize-error": "^13.0.1",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "dotenv": "^17.2.4",
    "pkgroll": "^2.23.1",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
